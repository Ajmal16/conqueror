<h1>Projects</h1>
  <% @projects.each do |project| %>

  <div class="card">
    <%= link_to project.name, project %>
     <p><%= project.description %></p>
      <%= link_to 'See Details', project, class: 'btn btn-primary' %>
  <% end %>
</div>
<%= link_to "New Project", new_project_path, class: "btn btn-success" %>
